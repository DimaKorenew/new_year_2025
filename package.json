{
  "name": "new-year-landing",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:client": "vite",
    "dev:ssr": "NODE_ENV=development node server/ssr.js",
    "build": "npm run build:client && npm run build:server && npm run build:prerender",
    "build:client": "tsc && vite build",
    "build:server": "tsc && vite build --config vite.config.ssr.ts",
    "build:prerender": "node scripts/prerender.js",
    "start": "vite preview",
    "preview": "vite preview",
    "preview:ssr": "NODE_ENV=production node server/ssr.js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.6",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  }
}
